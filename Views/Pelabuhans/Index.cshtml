            <th>
                @Html.DisplayNameFor(model => model.DeletedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeletedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoftDelete)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Baseline)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
