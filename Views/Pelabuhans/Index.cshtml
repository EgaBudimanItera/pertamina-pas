                @Html.DisplayFor(modelItem => item.DeletedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeletedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoftDelete)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Baseline)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idlistpelabuhan">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idlistpelabuhan">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idlistpelabuhan">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
