@model pas_pertamina.Models.ViewShipmenDetail

@{
    ViewData["Title"] = "Create";
}



<ol class="breadcrumb">
    <li><a href="#">Planner</a></li>
    <li class="active">Simulasi Planner</li>
</ol>
<div class="row">
    <div class="col-md-12">
        <div class="bgc-white bd bdrs-3 p-20 mB-20">
            <h4 class="c-grey-900 mB-20">Simulasi Planner</h4>
            <div class="alert alert-danger">
                <strong>Perhatian, </strong>pilih terlebih dahulu kapal, asal, tujuan, produk, jumlah produk, satuan sebelum memilih waktu datang
            </div>
            <div id="dataTable_wrapper" class="dataTables_wrapper">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <button type="submit" class="btn btn-primary pull-right">Simpan</button>
                    <div class="table-responsive">
                        <table border="0" cellpadding="1" cellspacing="1">
                            <tr>
                                <td>
                                    <select asp-for="Idkapal" class="form-control" asp-items="ViewBag.Idkapal">
                                        <option value="">Pilih Kapal</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <select asp-for="Idasal" class="form-control" asp-items="ViewBag.Idasal">
                                        <option value="">Pilih Asal Pelabuhan</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <select asp-for="Idtujuan" class="form-control" asp-items="ViewBag.Idtujuan">
                                        <option value="">Pilih Tujuan Pelabuhan</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <select asp-for="Proses" class="form-control">
                                        <option value="">Pilih Proses</option>
                                        <option value="0">Loading</option>
                                        <option value="1">Discharge</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <select asp-for="Nojetty" class="form-control">
                                        <option value="">Pilih Jetty</option>
                                        <option value="1">Jetty 1</option>
                                        <option value="2">Jetty 2</option>
                                        <option value="3">Jetty 3</option>
                                    </select>
                                </td>

                            </tr>
                            <tr>
                                <td colspan="9">
                                    &nbsp;
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <select asp-for="Idproduk" class="form-control" asp-items="ViewBag.Idproduk">
                                        <option value="">Pilih Produk</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <input asp-for="Jumlah" placeholder="Jumlah Produk" class="form-control" />
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <select asp-for="Idsatuan" class="form-control" asp-items="ViewBag.Idsatuan">
                                        <option value="">Pilih Satuan</option>
                                    </select>
                                </td>
                                <td>&nbsp;</td>
                                <td>
                                    <button class="btn btn-success pull-right">Simpan Produk</button>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <br />
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped" role="grid" style="width: 100%">
                            <thead>
                                <tr role="row">
                                    <th>
                                        Arrival
                                    </th>
                                    <th>
                                        Berthed
                                    </th>
                                    <th>
                                        Comm
                                    </th>
                                    <th>
                                        Comp
                                    </th>
                                    <th>
                                        Unberthed
                                    </th>
                                    <th>
                                        Departure
                                    </th>
                                    <th>
                                        IPT &nbsp; &nbsp;
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <input asp-for="Arrival" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="Berthed" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="Comm" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="Comp" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="Unberthed" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="Departure" class="form-control" />
                                    </td>
                                    <td>
                                        <input asp-for="ipt" class="form-control" value="00:00:00" readonly />
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </form>
                <div id="dataproyeksi">
                    <div id="loader-proyeksi" style="display:none">Memproses Proyeksi...</div>
                    <div id="result_proyeksi_stok_asal"></div>
                    <div id="result_proyeksi_stok_tujuan"></div>
                </div>
                <hr />
            </div>
        </div>
    </div>
</div>

