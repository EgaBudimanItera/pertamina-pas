        function updatewaktu(id, proses, nojetty) {
            var _Arrival = $('#_arrival_').val();
            var _Berthed = $('#_berthed_').val();
            var _Comm = $('#_comm_').val();
            var _Comp = $('#_comp_').val();
            var _Unberthed = $('#_unberthed_').val();
            var _Departure = $('#_departure_').val();
            console.log(id, proses, nojetty, _Arrival, _Berthed, _Comm, _Comp, _Unberthed, _Departure);
            
            $.ajax({
                url: "/Home/Update_Waktu",
                data: 'idshipment=' + id + '&nojetty=' + nojetty + '&proses=' + proses +
                    '&arrival=' + _Arrival + '&berthed=' + _Berthed + '&comm=' + _Comm + '&comp=' + _Comp + '&unberthed=' + _Unberthed +
                    '&departure=' + _Departure,
                type: "POST",
                dataType: 'JSON',
                success: function (msg) {
                    console.log(msg);
                    if (msg.status == 'sukses') {
                        alert('berhasil diupdate');
                        location.reload();
                    } else if (msg.status == 'gagal') {
                        alert('gagal diupdate');
                    }
                }
            });
        }

