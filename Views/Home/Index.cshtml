                                    </div>
                                    <div class="col-md-12">
                                        Antrian : @portschedule.Antrian , Jetty : @portschedule.Nojetty
                                    </div>
                                    <div class="col-md-12">
                                        @{ if (_akses == "Petugas BBM" || _akses == "Petugas LPG")
